#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules/markdown-pdf/bin/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules/markdown-pdf/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules/markdown-pdf/bin/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules/markdown-pdf/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/markdown-pdf@11.0.0/node_modules:/Users/acring/Workspace/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/markdown-pdf" "$@"
else
  exec node  "$basedir/../../bin/markdown-pdf" "$@"
fi
